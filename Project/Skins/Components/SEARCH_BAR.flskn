<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Search Bar skin"
  ComponentType="SearchBar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Search Bar">
        <Properties>
          <Property
            Name="BorderTop">solid 1px #555555</Property>
          <Property
            Name="BorderRight">solid 1px #555555</Property>
          <Property
            Name="BorderBottom">solid 1px #555555</Property>
          <Property
            Name="BorderLeft">solid 1px #555555</Property>
          <Property
            Name="Height">25px</Property>
          <Property
            Name="MarginTop">10px</Property>
          <Property
            Name="BorderTopLeftRadius">5px</Property>
          <Property
            Name="BorderTopRightRadius">5px</Property>
          <Property
            Name="BorderBottomRightRadius">5px</Property>
          <Property
            Name="BorderBottomLeftRadius">5px</Property>
          <Property
            Name="MarginLeft">10px</Property>
          <Property
            Name="MarginRight">10px</Property>
          <Property
            Name="Width">90%</Property>
          <Property
            Name="MarginBottom">12px</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button">
        <Properties>
          <Property
            Name="BackgroundColor">#555555</Property>
          <Property
            Name="Width">32px</Property>
          <Property
            Name="BackgroundImage">url('SearchIcon_white.png')</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Input">
        <Properties>
          <Property
            Name="Color">#555555</Property>
          <Property
            Name="FontSize">10pt</Property>
          <Property
            Name="BackgroundColor">#ffffff</Property>
          <Property
            Name="BackgroundGradient">vertical #ffffff #ffffff</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="SearchIcon_white.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings />
</CatapultSkin>